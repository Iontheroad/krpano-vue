	<scene name="scene_10006" title="10006" onstart="" thumburl="panos/10006.tiles/thumb.jpg" lat="34.71486111" lng="113.51763333" heading="214.3">
		
		<control bouncinglimits="calc:image.cube ? true : false" />

		<view hlookat="214.3" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/10006.tiles/preview.jpg" />

		<image prealign="0|214.3|0">
			<cube url="panos/10006.tiles/%s/l%l/%v/l%l_%s_%v_%h.jpg" multires="512,1024,2176" />
		</image>



		<!-- 可拖拽热点 -->
		<hotspot
			name="spot1546933175342"
			ath="-79.03260129263748"
			atv="11.32974335214361"
			linkedscene="scene_1"
			style="skin_hotspotstyle"
			ondown="draghotspot();"
		/>
		<plugin name="hotspot_pos_info"
			type="text"
			html="drag the hotspots..."
			css="font-family:Courier;font-size: 16px;"
			padding="8"
			align="lefttop" x="100" y="10"
			width="300"
			height="100"
			enabled="true"
		/>

	</scene>

